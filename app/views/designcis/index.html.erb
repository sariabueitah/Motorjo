<p id="notice"><%= notice %></p>

<h1>Listing Designcis</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @designcis.each do |designci| %>
      <tr>
        <td><%= designci.title %></td>
        <td><%= link_to 'Show', designci %></td>
        <td><%= link_to 'Edit', edit_designci_path(designci) %></td>
        <td><%= link_to 'Destroy', designci, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Designci', new_designci_path %>
