<p id="notice"><%= notice %></p>

<h1>Listing Interior Designs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interior_designs.each do |interior_design| %>
      <tr>
        <td><%= interior_design.title %></td>
        <td><%= link_to 'Show', interior_design %></td>
        <td><%= link_to 'Edit', edit_interior_design_path(interior_design) %></td>
        <td><%= link_to 'Destroy', interior_design, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interior design', new_interior_design_path %>
