<section class="PageHead">
  <div class="AdContainer">
    <div class="row">
      <div class="columns large-12 medium-12 medium-centered large-centered">
        <div class="AdArea">
            <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
            <!-- ????? -->
            <ins class="adsbygoogle"
                 style="display:inline-block;width:728px;height:90px"
                 data-ad-client="ca-pub-9051649943133421"
                 data-ad-slot="1612164192"></ins>
            <script>
            (adsbygoogle = window.adsbygoogle || []).push({});
            </script>
          </div>
      </div>
    </div>
  </div>
</section>
<section class="PageTitle">
  <div class="row">
    <div class="columns large-12 medium-12 medium-centered large-centered">
      <h2>Admin Panel</h2>
    </div>
  </div>
</section>
<section class="admin">
  <div class="row">
    <div class="columns large-12 medium-12 medium-centered large-centered">
      <p id="notice"><%= notice %></p>

      <h1>Listing Admin Main Sliders</h1>

      <table>
        <thead>
          <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Order</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @admin_main_sliders.each do |admin_main_slider| %>
            <tr>
              <td><%= admin_main_slider.title %></td>
              <td><%= admin_main_slider.description %></td>
              <td><%= admin_main_slider.order %></td>
              <td><%= link_to 'Show', admin_main_slider %></td>
              <td><%= link_to 'Edit', edit_admin_main_slider_path(admin_main_slider) %></td>
              <td><%= link_to 'Destroy', admin_main_slider, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>

      <br>

      <%= link_to 'New Main slider', new_admin_main_slider_path %>
    </div>
  </div>
</section>
